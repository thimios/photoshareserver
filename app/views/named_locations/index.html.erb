<h1>Listing named_locations</h1>

<table>
  <tr>
    <th>Reference</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @named_locations.each do |named_location| %>
  <tr>
    <td><%= named_location.reference %></td>
    <td><%= link_to 'Show', named_location %></td>
    <td><%= link_to 'Edit', edit_named_location_path(named_location) %></td>
    <td><%= link_to 'Destroy', named_location, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

