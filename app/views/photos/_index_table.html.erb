<table>
  <tr>
    <th>id</th>
    <th>Title</th>
    <th>Description</th>
    <th>Votes</th>
    <th>Category</th>
    <th>Posted by</th>
    <th>Voted by current user</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.id %></td>
        <td><%= photo.title %></td>
        <td><%= photo.description %></td>
        <td><%= photo.plusminus %></td>
        <td><%= photo.category.title %></td>
        <td><%= link_to photo.user.username, '/users/'+ photo.user.id.to_s %></td>
        <td><%= photo.voted_by_current_user %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, confirm: 'Are you sure?', method: :delete %></td>
      </tr>
  <% end %>
</table>

<%= paginate @photos %>
