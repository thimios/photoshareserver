<p>
  <b>Title:</b>
  <%= @photo.title %>
</p>

<p>
  <b>Description:</b>
  <%= @photo.description %>
</p>

<p>
  <b>Category:</b>
  <%= @photo.category.title %>
</p>

<p>
  <b>Address:</b>
  <%= @photo.address %>
</p>

<p>
  <b>Votes:</b>
  <%= @photo.plusminus %>
</p>

<p>
  <b>Voted by current user:</b>
  <%= @photo.voted_by_current_user %>
</p>

<%= form_tag(photo_path+"/vote_up", :method => "get") do %>
    <%= submit_tag("Vote up") %>
<% end %>

<%= comments_for @photo %>

<%= image_tag @photo.image.url %>
<%= image_tag @photo.image.url(:medium) %>
<%= image_tag @photo.image.url(:thumb) %>
<%= image_tag @photo.image.url(:full) %>

<%= gmaps4rails(@googleMapsJson) %>


<%= link_to 'Edit', edit_photo_path(@photo) %> |
<%= link_to 'Back', photos_path %>
